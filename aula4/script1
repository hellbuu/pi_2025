def conceito(nota) -> str:
    if (nota >= 8):
        res = "A"
    elif (nota >= 7):
        res = "B"
    elif (nota >= 6):
        res = "C"
    elif (nota >= 5):
        res = "D"
    else:
        res = "F"
    print(f'{res}')
        
def main():
    nota = float(input())
    conceito(nota)
    
main()
        
